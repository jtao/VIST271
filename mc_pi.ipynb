      "source": [
        "import random\n",
        "\n",
        "inside = 0\n",
        "nsamples = 1000\n",
        "\n",
        "for i in range(nsamples):\n",
        "    x = random.random();\n",
        "    y = random.random();\n",
        "    if (x*x)+(y*y)<1:\n",
        "        inside += 1\n",
        "\n",
        "pi = (4.0 * inside)/nsamples\n",
        "print (\"Computed value of pi is\",pi)"
      ]
